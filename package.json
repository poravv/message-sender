{
    "name": "message-sender",
    "version": "1.0.0",
    "description": "Sistema de env√≠o masivo de mensajes por WhatsApp",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js",
        "build": "echo 'No build step required'",
        "docker:build": "docker build -t whatsapp-sender .",
        "docker:run": "docker run -p 3000:3000 --env-file .env whatsapp-sender",
        "docker:compose": "docker-compose up -d",
        "docker:logs": "docker-compose logs -f audio-sender",
        "docker:stop": "docker-compose down",
        "docker:restart": "docker-compose restart audio-sender",
        "prod:start": "./start-prod.sh",
        "prod:stop": "docker-compose down",
        "prod:logs": "docker-compose logs -f audio-sender"
    },
    "keywords": [
        "whatsapp",
        "messages",
        "bulk",
        "sender"
    ],
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@hapi/boom": "^10.0.1",
        "@whiskeysockets/baileys": "^6.7.19",
        "ioredis": "^5.4.1",
        "cors": "^2.8.5",
        "csv-parser": "^3.2.0",
        "dotenv": "^16.5.0",
        "express": "^4.21.2",
        "express-jwt": "^8.5.1",
        "fluent-ffmpeg": "^2.1.3",
        "jose": "^6.0.13",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.2.0",
        "multer": "^1.4.5-lts.1",
        "pino": "^9.9.4",
        "pino-pretty": "^13.1.1",
        "qrcode": "^1.5.4",
        "qrcode-terminal": "^0.12.0"
    },
    "devDependencies": {
        "nodemon": "^3.1.9"
    },
